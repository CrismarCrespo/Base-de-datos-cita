CREATE DATABASE GESTION_CITAS
GO

USE GESTION_CITAS
GO

CREATE TABLE PACIENTES
(	
	CEDULA CHAR (8) PRIMARY KEY,
	NOMBRES VARCHAR (30),
	APELLIDOS VARCHAR (30),
	DIRECCION VARCHAR (30),
	TELEFONO VARCHAR (30),
);

CREATE TABLE ESPECIALIDADES
(
	ID INT IDENTITY(1, 1) PRIMARY KEY, 
	NOMBRE  VARCHAR(40)
);
